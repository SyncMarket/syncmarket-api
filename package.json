{
    "name": "syncmarket-api",
    "version": "1.0.0",
    "repository": "https://github.com/reisquaza/syncmarket-api",
    "author": "Jo√£o Victor Reis <joaovictor.reis2000@gmail.com>",
    "license": "MIT",
    "scripts": {
        "start": "node dist/lib/index.js",
        "build": "rimraf dist && tsc -p tsconfig-build.json",
        "test": "jest --detectOpenHandles --passWithNoTests --runInBand",
        "dev": "nodemon -L --watch ./dist ./dist/main/server.js",
        "build:watch": "yarn build -- --watch",
        "up": "concurrently --kill-others-on-fail \"yarn build:watch\" \"docker-compose up\""
    },
    "devDependencies": {
        "@babel/cli": "^7.23.4",
        "@babel/core": "^7.23.7",
        "@babel/node": "^7.22.19",
        "@babel/preset-env": "^7.23.8",
        "@babel/preset-typescript": "^7.23.3",
        "@types/jest": "^29.5.11",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "babel-plugin-module-resolver": "^5.0.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "jest": "^29.7.0",
        "nodemon": "^3.0.3",
        "prettier": "^3.2.4",
        "ts-jest": "^29.1.1",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "dotenv": "^16.3.2",
        "mongodb": "^6.3.0",
        "zod": "^3.22.4"
    },
    "_moduleAliases": {
        "@application": "dist/application",
        "@infra": "dist/infra",
        "@main": "dist/main",
        "@core": "dist/core",
        "@test": "dist/__test__"
    }
